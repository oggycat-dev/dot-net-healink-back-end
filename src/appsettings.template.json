{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionConfig": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=service_db;Username=db_user;Password=db_password"
  },
  "JwtConfig": {
    "Key": "your_jwt_secret_key",
    "Issuer": "Healink",
    "Audience": "Healink.Users",
    "ExpiresInMinutes": 60
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "healink_exchange"
  },
  "Redis": {
    "Host": "localhost",
    "Port": 6379,
    "Password": "your_redis_password",
    "DefaultDatabase": 0,
    "InstanceName": "Healink:"
  },
  "ServiceUrls": {
    "AuthService": "http://localhost:5001",
    "UserService": "http://localhost:5002",
    "ContentService": "http://localhost:5003",
    "NotificationService": "http://localhost:5004",
    "Gateway": "http://localhost:5010"
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "Port": 587,
    "Username": "your-email@gmail.com",
    "Password": "your-app-password",
    "FromEmail": "noreply@healink.com",
    "FromName": "Healink System"
  },
  "CORS": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://healink.com"
    ]
  },
  "AwsS3Config": {
    "AccessKey": "your_aws_access_key",
    "SecretKey": "your_aws_secret_key",
    "Region": "ap-southeast-1",
    "BucketName": "your-s3-bucket-name"
  }
}
