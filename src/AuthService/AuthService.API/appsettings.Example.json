{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionConfig": {
    "DefaultConnection": "Host=localhost;Database=authservicedb;Username=your_db_user;Password=your_db_password",
    "RetryOnFailure": true,
    "MaxRetryCount": 3,
    "MaxRetryDelay": 30,
    "ErrorNumbersToAdd": []
  },
  "JwtConfig": {
    "Key": "your_jwt_secret_key_must_be_at_least_32_characters_long",
    "Issuer": "Healink",
    "Audience": "Healink.Users",
    "ExpiresInMinutes": 60,
    "RefreshTokenExpiresInDays": 7,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true,
    "ClockSkewMinutes": 5
  },

  "DefaultAdminAccount": {
    "Email": "admin@healink.com",
    "Password": "your_admin_password",
    "UserId": "your-admin-user-id-guid"
  },

  "CorsConfig": {
    "PolicyName": "DefaultCors",
    "AllowAnyOrigin": true,
    "AllowAnyMethod": true,
    "AllowAnyHeader": true,
    "AllowCredentials": true,
    "AllowedOrigins": "localhost3000, localhost3001, localhost:5173, localhost:4200",
    "AllowedMethods": "*",
    "AllowedHeaders": "*",
    "ExposedHeaders": "Content-Disposition, Token-Expired",
    "PreflightMaxAgeSeconds": 600
  },
  "DataConfig": {
    "EnableSeeding": true,
    "EnableAutoApplyMigrations": true
  },
  "RabbitMQ": {
    "HostName": "rabbitmq",
    "Port": 5672,
    "UserName": "your_rabbitmq_user",
    "Password": "your_rabbitmq_password",
    "VirtualHost": "/",
    "ExchangeName": "healink_exchange",
    "QueueName": "authservice_queue",
    "Durable": true,
    "AutoDelete": false,
    "RetryCount": 3,
    "RetryDelaySeconds": 5,
    "AutoAck": false,
    "PrefetchCount": 10,
    "ConnectionTimeoutSeconds": 30,
    "HeartbeatIntervalSeconds": 60,
    "UseSsl": false,
    "SslServerName": ""
  },
  "Cache": {
    "UserStateCacheMinutes": 30,
    "UserStateSlidingMinutes": 15,
    "ActiveUsersListHours": 24,
    "CleanupIntervalMinutes": 60,
    "MaxCacheSize": 10000
  },
  "OutboxProcessor": {
    "ProcessingIntervalSeconds": 30,
    "BatchSize": 50,
    "MaxRetryAttempts": 5,
    "Enabled": true,
    "ProcessingTimeoutSeconds": 300,
    "EnableRetryWithBackoff": true
  },
  "PasswordEncryptKey": "your_password_encrypt_key",
  "OTPSettings": {
    "Length": 6,
    "ExpirationMinutes": 5,
    "MaxAttempts": 3
  }
}