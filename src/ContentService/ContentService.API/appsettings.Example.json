{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=healink_content_db;Username=healink_user;Password=HealinkSecure2024!"
  },
  "AllowedHosts": "*",
  "AWS": {
