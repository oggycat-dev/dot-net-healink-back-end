{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ocelot": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtConfig": {
    "Key": "GatewaySecretKeyIsLongEnoughToBeUsedWithJWT",
    "Issuer": "ProductAuthMicroservice",
    "Audience": "ProductAuthMicroservice.Users",
    "ExpireInMinutes": 60,
    "RefreshTokenExpireInDays": 7,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true,
    "ClockSkewMinutes": 5
  },
  "Cache": {
    "UserStateCacheMinutes": 30,
    "UserStateSlidingMinutes": 15,
    "ActiveUsersListHours": 24,
    "CleanupIntervalMinutes": 60,
    "MaxCacheSize": 10000
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "product_auth_exchange",
    "QueueName": "",
    "Durable": true,
    "AutoDelete": false,
    "RetryCount": 3,
    "RetryDelaySeconds": 5,
    "AutoAck": false,
    "PrefetchCount": 10,
    "ConnectionTimeoutSeconds": 30,
    "HeartbeatIntervalSeconds": 60,
    "UseSsl": false,
    "SslServerName": ""
  },
  "OutboxProcessor": {
    "ProcessingIntervalSeconds": 30,
    "BatchSize": 50,
    "MaxRetryAttempts": 5,
    "Enabled": true,
    "ProcessingTimeoutSeconds": 300,
    "EnableRetryWithBackoff": true
  }
}